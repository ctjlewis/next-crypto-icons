{
  "name": "next-crypto-icons",
  "homepage": "https://github.com/ctjlewis/next-crypto-icons",
  "author": "C. Lewis <ctj.lewis@icloud.com>",
  "type": "module",
  "version": "1.0.2",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "lint": "next lint --fix",
    "dev": "next dev",
    "start": "next start",
    "export": "tsmodule build",
    "build": "next build && yarn export",
    "test": "yarn build && ava",
    "prepublishOnly": "yarn test"
  },
  "module": "./dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./*": "./dist/components/*/index.js",
    "./styles": "./dist/bundle.css",
    "./styles/*": "./dist/components/*/index.css"
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=16"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@tsmodule/react": "^12.0.0",
    "@tsmodule/tsmodule": "^40.0.17",
    "@types/node": "^18.7.3",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "autoprefixer": "^10.4.8",
    "ava": "^4.3.1",
    "cssnano": "^5.1.13",
    "eslint": "^8.22.0",
    "eslint-config-next": "^12.2.5",
    "husky": "^7.0.4",
    "next": "^12.2.5",
    "postcss": "^8.4.16",
    "postcss-import": "^14.1.0",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "yarn-deduplicate": "^3.1.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "platform": "browser",
  "types": "dist/index.d.ts",
  "ava": {
    "timeout": "10m",
    "files": [
      "test/**/*.test.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--no-warnings",
      "--loader=@tsmodule/tsmodule/loader"
    ]
  },
  "style": "dist/bundle.css",
  "resolutions": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11"
  }
}